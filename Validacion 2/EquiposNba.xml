<!DOCTYPE EquiposNBA [
    <!ELEMENT EquiposNBA (equipo | nombre)+>
    
    <!ELEMENT nombre (#PCDATA)>
    
    <!ELEMENT equipo (titulos, posicion, quinteto)>
    <!ATTLIST equipo conferencia CDATA #REQUIRED>
    
